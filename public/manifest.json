{
  "short_name": "Homes API",
  "manifest_version": 2,
  "name": "Homes API extension",
  "description": "This is a Homes API for Chrome extension",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Homes API"
  },
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "permissions": ["<all_urls>", "http://*/*", "https://*/*", "tabs"]
}
